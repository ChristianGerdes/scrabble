{
    "version": "0.2.0",
    "configurations": [
      {
        "name": ".NET Core Launch (console)",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build",
        "program": "${workspaceFolder}/ScrabbleTemplate/bin/Debug/net5.0/ScrabbleTemplate.dll",
        "args": [],
        "cwd": "${workspaceFolder}",
        "console": "externalTerminal",
        "stopAtEntry": false,
        "internalConsoleOptions": "openOnSessionStart"
      },
      {
        "name": ".NET Core Attach",
        "type": "coreclr",
        "request": "attach",
        "processId": "${command:pickProcess}"
      }
    ]
  }